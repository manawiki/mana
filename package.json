{
   "private": true,
   "sideEffects": false,
   "scripts": {
      "dev": "run-p dev:core dev:custom",
      "build": "yarn run generate:types && run-p \"build:core:*\" && run-p \"build:custom:*\"",
      "start": "run-p start:core start:custom",
      "dev:core": "yarn run generate:types && remix dev -c \"npm run dev:node\" --manual",
      "dev:node": "cross-env NODE_ENV=development PAYLOAD_CONFIG_PATH=./app/db/payload.config.ts node -r ts-node/register ./core.server.ts",
      "dev:custom": "cross-env NODE_ENV=development PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts node -r ts-node/register ./custom.server.ts",
      "build:core": "yarn run generate:types && run-p \"build:core:*\"",
      "build:core:remix": "remix build",
      "build:core:payload": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.config.ts payload build",
      "build:core:server": "tsc -p tsconfig.server.json",
      "build:custom": "yarn run generate:types && run-p \"build:custom:*\"",
      "build:custom:payload": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts payload build",
      "build:custom:server": "tsc -p tsconfig.custom.json",
      "start:core": "cross-env PAYLOAD_CONFIG_PATH=build/core/app/db/payload.config.js NODE_ENV=production node build/core/core.server.js",
      "start:custom": "cross-env PAYLOAD_CONFIG_PATH=build/custom/app/db/payload.custom.config.js NODE_ENV=production node build/custom/custom.server.js",
      "typecheck": "tsc -p tsconfig.json --diagnostics",
      "partytown": "partytown copylib public/~partytown",
      "generate:icons": "npx @sly-cli/sly add lucide-icons --overwrite --yes --directory ./public/icons",
      "generate:types": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.config.ts yarn generate:types:clean",
      "generate:types:custom": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts yarn generate:types:clean",
      "generate:types:clean": "payload generate:types && node ./scripts/clean-payload-types.js",
      "clean": "shx rm -rf node_modules dist build public/build",
      "seed:core": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.config.ts ts-node -T app/_custom/seed/index.ts",
      "seed:custom": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts ts-node -T app/_custom/seed/index.ts",
      "resave": "cross-env PAYLOAD_CONFIG_PATH=./app/db/payload.custom.config.ts ts-node -T ./scripts/resaveCollection",
      "postinstall": "patch-package"
   },
